Cell[BoxData[{
 RowBox[{
  RowBox[{"ClearAll", "[", "MultivariateCoupledDistribution", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"MultivariateCoupledDistribution", "[", 
   RowBox[{
   "\[Mu]_", ",", "\[CapitalSigma]_", ",", "\[Kappa]_", ",", "\[Alpha]_"}], 
   "]"}], ":=", " ", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"MultivariateCoupledDistribution", "[", 
    RowBox[{
    "\[Mu]", ",", "\[CapitalSigma]", ",", "\[Kappa]", ",", "\[Alpha]"}], 
    "]"}], "=", "\[IndentingNewLine]", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"dimMean", ",", "dimCor", ",", "x"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Check", " ", "that", " ", "\[CapitalSigma]", " ", "is", " ", "a", " ", 
       "positive", " ", "definite", " ", "matrix", " ", "with", " ", 
       "dimensions", " ", "equal", " ", "to", " ", "length", " ", "of", " ", 
       "\[Mu]"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"dimMean", "=", 
          RowBox[{"Length", "[", "\[Mu]", "]"}]}], ";", 
         RowBox[{"dimCor", "=", 
          RowBox[{"Dimensions", "[", "\[CapitalSigma]", "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"dimCor", "\[NotEqual]", "dimMean"}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{"Message", "[", 
            RowBox[{
             RowBox[{"MultivariateCoupledDistribution", "::", "argx"}], ",", 
             "dimCor", ",", "dimMean"}], "]"}]}], "\[IndentingNewLine]", 
          "]"}], ";"}], "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"Not", "@", 
             RowBox[{
             "PositiveDefiniteMatrixQ", "[", "\[CapitalSigma]", "]"}]}], ",", 
            "\[IndentingNewLine]", 
            RowBox[{"Message", "[", 
             RowBox[{"MultivariateCoupledDistribution", "::", "corr"}], 
             "]"}]}], "\[IndentingNewLine]", "]"}], ";"}], "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"(*", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"\[Alpha]", "\[NotEqual]", 
             RowBox[{"(", 
              RowBox[{"1", "||", "2"}], ")"}]}], ",", "\[IndentingNewLine]", 
            RowBox[{"Message", "[", 
             RowBox[{
              RowBox[{"MultivariateCoupledDistribution", "::", "alpha"}], ",",
               "\[Alpha]"}], "]"}]}], "\[IndentingNewLine]", "]"}], ";"}], 
         "\[IndentingNewLine]", "*)"}], "\[IndentingNewLine]", " ", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{"Define", " ", "the", " ", "distribution"}], " ", "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
         "Evaluation", " ", "of", " ", "the", " ", "input", " ", "x", " ", 
          "to", " ", "CoupledExponential", " ", "is", " ", "completed", " ", 
          "to", " ", "insure", " ", "that", " ", "the", " ", "definition", " ",
           "of", " ", "CoupledExponential", " ", "can", " ", "stay", " ", 
          "one", " ", "dimensional"}], " ", "*)"}], "\[IndentingNewLine]", 
        "x"}], "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"Symbol", "[", 
          RowBox[{"\"\<$x\>\"", "<>", 
           RowBox[{"ToString", "@", "i"}]}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "dimMean"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"ProbabilityDistribution", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"CoupledExponential", "[", 
              RowBox[{
               SuperscriptBox[
                RowBox[{"(", 
                 RowBox[{
                  RowBox[{"(", 
                   RowBox[{"x", "-", "\[Mu]"}], ")"}], ".", 
                  RowBox[{"Inverse", "[", "\[CapitalSigma]", "]"}], ".", 
                  RowBox[{"(", 
                   RowBox[{"x", "-", "\[Mu]"}], ")"}]}], ")"}], 
                FractionBox["\[Alpha]", "2"]], ",", "\[Kappa]", ",", 
               "dimMean"}], "]"}], ")"}], 
            FractionBox[
             RowBox[{"-", "1"}], "\[Alpha]"]], "/", 
           RowBox[{"NormMultiCoupled", "[", 
            RowBox[{
             SqrtBox[
              RowBox[{"Det", "[", "\[CapitalSigma]", "]"}]], ",", "\[Kappa]", 
             ",", "\[Alpha]", ",", "dimMean"}], "]"}]}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{"(*", " ", 
           RowBox[{"Define", " ", "the", " ", "domain"}], " ", "*)"}], 
          "\[IndentingNewLine]", "##"}], "\[IndentingNewLine]", "]"}], "&"}], 
       "@@", "\[IndentingNewLine]", 
       RowBox[{"Which", "[", 
        RowBox[{
         RowBox[{"\[Alpha]", "\[Equal]", "1"}], ",", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{
             "x", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], ",", 
             RowBox[{
             "\[Mu]", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], 
             ",", "\[Infinity]"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", "dimMean"}], "}"}]}], "]"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"\[Alpha]", "\[Equal]", "2"}], ",", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{
             "x", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], ",", 
             RowBox[{"-", "\[Infinity]"}], ",", "\[Infinity]"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", "dimMean"}], "}"}]}], "]"}]}], 
        "\[IndentingNewLine]", "]"}]}]}]}], " ", "\[IndentingNewLine]", 
    "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
