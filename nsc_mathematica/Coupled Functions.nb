 RowBox[{
  RowBox[{"ClearAll", "[", "CoupledExponentialDistribution", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"CoupledExponentialDistribution", "[", 
   RowBox[{
    RowBox[{"\[Mu]_", ":", "0"}], ",", "\[Sigma]_", ",", "\[Kappa]_"}], "]"}],
   ":=", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"CoupledExponentialDistribution", "[", 
    RowBox[{"\[Mu]", ",", "\[Sigma]", ",", "\[Kappa]"}], "]"}], "=", 
   "\[IndentingNewLine]", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "x", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"ProbabilityDistribution", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        FractionBox["1", "\[Sigma]"], 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"CoupledExponential", "[", 
           RowBox[{
            FractionBox[
             RowBox[{"x", "-", "\[Mu]"}], "\[Sigma]"], ",", "\[Kappa]", ",", 
            "1"}], "]"}], ")"}], 
         RowBox[{"-", "1"}]]}], ",", "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{"x", ",", "\[Mu]", ",", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"\[Kappa]", "\[GreaterEqual]", "0"}], ",", "\[Infinity]", ",", 
           RowBox[{
            FractionBox[
             RowBox[{"-", "\[Sigma]"}], "\[Kappa]"], "+", "\[Mu]"}]}], 
          "]"}]}], "}"}]}], "\[IndentingNewLine]", "]"}]}], 
    "\[IndentingNewLine]", "]"}]}]}]}], "Input",
