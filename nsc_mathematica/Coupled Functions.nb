Cell[TextData[{
 "qToCoupling and couplingToq\nThe q-Gaussian is defined as ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     SqrtBox["\[Beta]"], 
     SubscriptBox["C", "q"]], 
    RowBox[{
     SubscriptBox["e", "q"], "(", 
     RowBox[{
      RowBox[{"-", 
       SubscriptBox["\[Beta]", "q"]}], 
      SuperscriptBox["x", "2"]}], ")"}]}], TraditionalForm]],ExpressionUUID->
  "1d9b3cfa-cc0d-4917-bbf0-44d2440f2652"],
 "; ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["e", "q"], "(", "y", ")"}], "=", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"1", "+", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"1", "-", "q"}], ")"}], "y"}]}], ")"}], 
      FractionBox["1", 
       RowBox[{"1", "-", "q"}]]]}], ";", " ", 
    RowBox[{
     SubscriptBox["C", "q"], "=", 
     RowBox[{
      SqrtBox[
       FractionBox["\[Pi]", 
        RowBox[{"q", "-", "1"}]]], 
      FractionBox[
       RowBox[{"Gamma", "[", 
        FractionBox[
         RowBox[{"3", "-", "q"}], 
         RowBox[{
          RowBox[{"2", "q"}], "-", "2"}]], "]"}], 
       RowBox[{"Gamma", "[", 
        FractionBox["1", 
         RowBox[{"q", "-", "1"}]], "]"}]]}]}]}], TraditionalForm]],
  ExpressionUUID->"ca1f6225-79b8-41be-a471-83dbe9727f56"]
