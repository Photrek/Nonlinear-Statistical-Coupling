Cell[BoxData[{
 RowBox[{
  RowBox[{"ClearAll", "[", "NormMultiCoupled", "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"NormMultiCoupled", "[", 
   RowBox[{
   "\[CapitalSigma]_", ",", "\[Kappa]_", ",", "\[Alpha]_", ",", "d_"}], "]"}],
   ":=", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"NormMultiCoupled", "[", 
    RowBox[{"\[CapitalSigma]", ",", "\[Kappa]", ",", "\[Alpha]", ",", "d"}], 
    "]"}], "=", "\[IndentingNewLine]", 
   RowBox[{"Which", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"\[Alpha]", "\[Equal]", "1"}], ",", 
     FractionBox[
      SuperscriptBox[
       RowBox[{"Det", "[", "\[CapitalSigma]", " ", "]"}], 
       RowBox[{"1", "/", "2"}]], 
      RowBox[{"1", "+", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"-", "1"}], "+", "d"}], ")"}], " ", "\[Kappa]"}]}]], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"\[Alpha]", "\[Equal]", "2"}], ",", 
     FractionBox[
      RowBox[{
       SqrtBox["\[Pi]"], " ", 
       SuperscriptBox[
        RowBox[{"Det", "[", "\[CapitalSigma]", " ", "]"}], 
        RowBox[{"1", "/", "2"}]], 
       RowBox[{"Gamma", "[", 
        FractionBox[
         RowBox[{"1", "+", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"-", "1"}], "+", "d"}], ")"}], " ", "\[Kappa]"}]}], 
         RowBox[{"2", " ", "\[Kappa]"}]], "]"}]}], 
      RowBox[{
       SqrtBox["\[Kappa]"], " ", 
       RowBox[{"Gamma", "[", 
        FractionBox[
         RowBox[{"1", "+", 
          RowBox[{"d", " ", "\[Kappa]"}]}], 
         RowBox[{"2", " ", "\[Kappa]"}]], "]"}]}]]}], "\[IndentingNewLine]", 
    "]"}]}]}]}], "Input",
