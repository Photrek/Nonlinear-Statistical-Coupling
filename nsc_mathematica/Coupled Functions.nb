  RowBox[{"CoupledExponentialDistribution", "[", 
   RowBox[{
    RowBox[{"\[Mu]_", ":", "0"}], ",", "\[Sigma]_", ",", "\[Kappa]_"}], "]"}],
   ":=", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"CoupledExponentialDistribution", "[", 
