3f9bdcb20bec"],

Cell[BoxData[
 RowBox[{"\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"Clear", "[", "CoupledVariate", "]"}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"CoupledVariate", "[", 
      RowBox[{
       RowBox[{"\[Mu]_", ":", "0"}], ",", 
       RowBox[{"\[Sigma]_", ":", "1"}], ",", "\[Kappa]_", ",", 
       RowBox[{"n_", ":", "1"}]}], "]"}], " ", ":=", 
     RowBox[{"Module", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"UniformVariates", "=", 
          RowBox[{"RandomReal", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"0", ",", "1"}], "}"}], ",", 
            RowBox[{"2", "n"}]}], "]"}]}], ",", "CoupledNormalVariates"}], 
        "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"CoupledNormalVariates", " ", "=", "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"n", "\[Equal]", "1"}], ",", "\[IndentingNewLine]", 
           RowBox[{
            SqrtBox[
             RowBox[{"CoupledLogarithm", "[", 
