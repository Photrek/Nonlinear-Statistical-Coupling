Cell[BoxData[{
 RowBox[{
  RowBox[{"ClearAll", "[", "CoupledExponential", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"CoupledExponential", "[", 
   RowBox[{"x_", ",", 
    RowBox[{"\[Kappa]_", ":", "0"}], ",", 
    RowBox[{"d_", ":", "1"}]}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"CoupledExponential", "[", 
    RowBox[{"x", ",", "\[Kappa]", ",", "d"}], "]"}], "=", 
   "\[IndentingNewLine]", 
   RowBox[{"If", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "+", 
        RowBox[{"\[Kappa]", " ", "x"}]}], ")"}], ">", "0"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"\[Kappa]", "\[NotEqual]", "0"}], ",", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"1", "+", 
          RowBox[{"\[Kappa]", " ", "x"}]}], ")"}], 
        FractionBox[
         RowBox[{"1", "+", 
          RowBox[{"d", " ", "\[Kappa]"}]}], 
         RowBox[{" ", "\[Kappa]"}]]], ",", 
       RowBox[{"Exp", "[", "x", "]"}]}], "]"}], ",", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        FractionBox[
         RowBox[{"1", "+", 
          RowBox[{"d", " ", "\[Kappa]"}]}], "\[Kappa]"], ">", "0"}], ",", "0",
        ",", "\[Infinity]"}], "]"}]}], "\[IndentingNewLine]", 
    "]"}]}]}]}], "Input",
