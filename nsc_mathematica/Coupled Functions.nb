  RowBox[{"NormCG", "[", 
   RowBox[{"\[Sigma]_", ",", "\[Kappa]_"}], "]"}], ":=", 
  TagBox[GridBox[{
     {"\[Piecewise]", GridBox[{
        {
         RowBox[{
          SqrtBox[
           RowBox[{"2", " ", "\[Pi]"}]], " ", "\[Sigma]"}], 
         RowBox[{"\[Kappa]", "\[Equal]", "0"}]},
        {
         FractionBox[
          RowBox[{
           SqrtBox["\[Pi]"], " ", "\[Sigma]", " ", 
